<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/azulOscuro"
    tools:context=".PantallaMenu">

    <ImageButton
        android:id="@+id/menu_btnMenu"
        android:layout_width="@dimen/menuButtonWidth"
        android:layout_height="@dimen/menuButtonHeight"
        android:layout_marginEnd="20dp"
        android:background="@color/boton"
        android:scaleType="centerCrop"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/menuwhite" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="150dp"
        android:background="@drawable/rounded_menu"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu_btnMenu"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/gothammedium"
            android:text="@string/menu"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView9"
            app:layout_constraintStart_toStartOf="@+id/imageView9"
            app:layout_constraintTop_toTopOf="@+id/imageView9" />

        <ImageButton
            android:id="@+id/menu_icon_informe"
            android:layout_width="@dimen/menuButtonWidth"
            android:layout_height="@dimen/menuButtonHeight"
            android:layout_marginRight="10dp"
            android:background="@color/boton"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toStartOf="@+id/mnu_informe"
            app:layout_constraintTop_toTopOf="@+id/mnu_informe"
            app:srcCompat="@drawable/documentoarchivowithe" />

        <TextView
            android:id="@+id/mnu_informe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/gothammedium"
            android:text="Informe SEP- COVID\nConfiguración"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView9"
            app:layout_constraintTop_toBottomOf="@+id/textView19" />


        <ImageButton
            android:id="@+id/menu_icon_config"
            android:layout_width="@dimen/menuButtonWidth"
            android:layout_height="@dimen/menuButtonHeight"
            android:layout_marginTop="20dp"
            android:background="@color/boton"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="@+id/menu_icon_informe"
            app:layout_constraintStart_toStartOf="@+id/menu_icon_informe"
            app:layout_constraintTop_toBottomOf="@+id/menu_icon_informe"
            app:srcCompat="@drawable/baseline_miscellaneous_services_white" />


        <TextView
            android:id="@+id/mnu_configuracion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/gothammedium"
            android:text="Configuración"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/menu_icon_config"
            app:layout_constraintStart_toEndOf="@+id/menu_icon_config"
            app:layout_constraintTop_toTopOf="@+id/menu_icon_config"
            app:layout_constraintVertical_bias="0.555" />

        <ImageButton
            android:id="@+id/menu_icon_ayuda"
            android:layout_width="@dimen/menuButtonWidth"
            android:layout_height="@dimen/menuButtonHeight"
            android:layout_marginTop="20dp"
            android:background="@color/boton"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="@+id/menu_icon_config"
            app:layout_constraintStart_toStartOf="@+id/menu_icon_config"
            app:layout_constraintTop_toBottomOf="@+id/menu_icon_config"
            app:srcCompat="@drawable/baseline_help_outline_white" />

        <TextView
            android:id="@+id/mnu_ayuda"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/gothammedium"
            android:text="Ayuda"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/menu_icon_ayuda"
            app:layout_constraintStart_toEndOf="@+id/menu_icon_ayuda"
            app:layout_constraintTop_toTopOf="@+id/menu_icon_ayuda"
            app:layout_constraintVertical_bias="0.444" />

        <ImageButton
            android:id="@+id/menu_icon_cerrar_sesion"
            android:layout_width="@dimen/menuButtonWidth"
            android:layout_height="@dimen/menuButtonHeight"
            android:layout_marginBottom="20dp"
            android:background="@color/boton"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="@+id/imageView9"
            app:layout_constraintEnd_toEndOf="@+id/menu_icon_ayuda"
            app:layout_constraintStart_toStartOf="@+id/menu_icon_ayuda"
            app:srcCompat="@drawable/baseline_clear_white" />

        <TextView
            android:id="@+id/mnu_cerrar_sesion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gothammedium"
            android:text="Cerrar sesión"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/menu_icon_cerrar_sesion"
            app:layout_constraintStart_toEndOf="@+id/menu_icon_cerrar_sesion"
            app:layout_constraintTop_toTopOf="@+id/menu_icon_cerrar_sesion" />
    </androidx.constraintlayout.widget.ConstraintLayout>





















</androidx.constraintlayout.widget.ConstraintLayout>